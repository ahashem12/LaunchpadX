{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "sourceMap": true,
    "paths": {
      "https://deno.land/std@0.177.0/http/server.ts": [
        "./node_modules/@types/deno/index.d.ts"
      ]
    }
  },
  "include": ["**/*.ts"]
}
